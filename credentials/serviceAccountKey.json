{
    "type": "service_account", // Indicates this is a service account credential
    "project_id": "your_project_id", // Google Cloud project ID
    "private_key_id": "your_private_key_id", // Unique identifier for the private key
    "private_key": "your_private_key", // Private key used for authentication
    "client_email": "your_client_email", // Service account email
    "client_id": "your_client_id", // Service account client ID
    "auth_uri": "your_auth_uri", // Authorization URI for OAuth2
    "token_uri": "your_token_uri", // Token URI for obtaining access tokens
    "auth_provider_x509_cert_url": "your_auth_provider_x509_cert_url", // Certificate URL for Google authentication
    "client_x509_cert_url": "your_client_cert_url" // Certificate URL for client authentication
}
